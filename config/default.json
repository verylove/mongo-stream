{
  "mongo": {
    "options": {
      "poolSize": 100
    },
    "url": "localhost:27017",
    "database": "test",
    "user": "admin",
    "collections": ["test"]
  },
  "elasticsearch": {
    "host": "localhost:9200",
    "apiVersion": "2.4"
  },
  "bulkSize": 1000,
  "adminPort": 3000,
  "ignoreResumeTokensOnStart": false,
  "说明":"全量同步开关",
  "dumpOnStart": false,
  "说明":"清空ES开关",
  "ignoreDumpProgress":false,
  "ignoreResumeTokensOnStart":false,
  "resumeTokenInterval": 60000,
  "resumeTokenCollection": "resumeToken",
  "dumpProgressCollection": "dumpProgress",
  "mappings": {
    "childCollectionName": {
      "index": "test",
      "type": "$self"
    }
  },
  "logLevel": "info"
}
